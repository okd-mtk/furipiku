<!--バリデーション-->

<h1>新規投稿</h1>
<%= form_with model: @picture, url: pictures_path,local: true do |f| %>

<!--タグ-->
タグ欄
<%= f.text_field :tag %>
<%= f.select :tag, options_from_collection_for_select(picture.tags, :id, :tags_display) %>

<!--文章-->
<%= f.text_field :explain %>

<!--イラスト-->
<%= f.file_field :images, multiple: :true %>

<% f.submit '投稿' %>

<% end %>